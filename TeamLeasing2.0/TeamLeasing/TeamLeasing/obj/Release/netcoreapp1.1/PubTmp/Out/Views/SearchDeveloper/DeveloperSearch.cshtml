 
@model  List<TeamLeasing.Models.DeveloperUser>
<div class="container" style="padding-top: 60px;">
    <div id="divWithBanner" class="row">
        @Html.Partial("_Banner")
    </div>
    <div class="row" style="padding-top: 20px;">
        <button style="margin-top: 00px; margin-bottom: 20px;" type="button" class="btn btn-info visible-xs visible-sm col-xs-12" data-toggle="collapse" data-target="#form">Wyszukiwarka</button>
        <div id="form" class="col-md-4 col-xs-12 collapse in" style="background-color: whitesmoke;">
            @await Component.InvokeAsync("SearchDeveloper")
        </div>
        <div class="col-md-8 col-xs-12" style="background-color: whitesmoke;  border: silver solid">
            @{
                if (Model != null)
                {
                    foreach (var developer in Model)
                    {
                        <div class="profile-item">
                            <img src=@Url.Content(developer.Photo) style="height: 233px; width: 233px;">
                            <div>
                                <table>
                                    <th><a asp-controller="SearchDeveloper" asp-action="Profile" asp-route-developerId="@developer.Id">@developer.Name @developer.Surname</a></th>
                                    <tr>
                                        <td>@developer.Technology.Name</td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>@developer.Level.ToString()</td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>@Html.LabelFor(l => developer.BirthDate)</td>
                                        <td>@developer.BirthDate.ToString("d")</td>
                                    </tr>
                                    <tr>
                                        <td>@Html.LabelFor(s => developer.Experience)</td>
                                        <td>@developer.Experience</td>
                                    </tr>
                                    <tr>
                                        <td>@Html.LabelFor(l => developer.University)</td>
                                        <td>@developer.University</td>
                                    </tr>
                                    <tr>
                                        <td>@Html.LabelFor(l => developer.City)</td>
                                        <td>@developer.City</td>
                                    </tr>
                                    <tr>
                                        <td>@Html.LabelFor(l => developer.Province)</td>
                                        <td>@developer.Province</td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    }
                }
            }
        </div>
    </div>
</div>
